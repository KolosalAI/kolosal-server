{    "server": {
        "port": "8081",
        "host": "0.0.0.0",
        "max_connections": 100,
        "worker_threads": 0,
        "request_timeout": 30,
        "max_request_size": 16777216,
        "idle_timeout": 300,
        "allow_public_access": false,
        "allow_internet_access": false
    },
    "logging": {
        "level": "INFO",
        "file": "",
        "access_log": false
    },
    "auth": {
        "enabled": false,
        "require_api_key": false,
        "api_key_header": "X-API-Key",
        "api_keys": [],
        "rate_limit": {
            "enabled": false,
            "max_requests": 100,
            "window_size": 60
        },
        "cors": {
            "enabled": true,
            "allow_credentials": false,
            "max_age": 86400,
            "allowed_origins": [
                "*"
            ],
            "allowed_methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS",
                "HEAD",
                "PATCH"
            ],
            "allowed_headers": [
                "Content-Type",
                "Authorization",
                "X-Requested-With",
                "Accept",
                "Origin"
            ]
        }
    },
    "models": [],
    "features": {
        "health_check": true,
        "metrics": false
    }
}
